	"jest": {
		"preset": "react-native",
		"cacheDirectory": "./cache",
		"setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
		"globals": {
			"window": {}
		},
		"coveragePathIgnorePatterns": [
			"./app/utils/vendor"
		],
		"coverageThreshold": {
			"global": {
				"statements": 29
			}
		},
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		],
		"transformIgnorePatterns": [
			"/node_modules/(?!react-native|@shoutem|react-clone-referenced-element|react-navigation|@valdio|@react-native-community|@figureai)"
		],
		"transform": {
			"^.+\\.js$": "<rootDir>/jest-preprocessor.js",
			"^.+\\.jsx?$": "babel-jest",
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/custom-file-loader.js"
		}
